i=1
while i<=10
   
x = imread(['subimage', num2str(i), '.jpg']);
a = imresize(x, [128, 128]); % 调整图像大小
label = classify(net, a);
% disp(['模型分类结果：', char(label)]);
disp(['subimage', num2str(i), '.jpg', num2str(i), ' 的模型分类结果：', char(label)]);
i=i+1;
end

